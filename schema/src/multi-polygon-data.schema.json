{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "multi-polygon-data.schema.json",
  "title": "MultiPolygonData",
  "type": "object",
  "required": [
    "type",
    "coordinates"
  ],
  "properties": {
    "type": {
      "const": "MultiPolygon",
      "default": "MultiPolygon"
    },
    "coordinates": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "$ref": "linear-ring-data.schema.json"
        }
      }
    }
  },
  "additionalProperties": false
}