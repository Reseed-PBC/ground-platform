{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "polygon-data.schema.json",
  "title": "PolygonData",
  "type": "object",
  "required": [
    "type",
    "coordinates"
  ],
  "properties": {
    "type": {
      "const": "Polygon",
      "default": "Polygon"
    },
    "coordinates": {
      "type": "array",
      "items": {
        "$ref": "linear-ring-data.schema.json"
      }
    }
  },
  "additionalProperties": false
}