{
  "name": "lib",
  "main": "dist/index.js",
  "description": "Common TypeScript libraries",
  "scripts": {
    "clean": "rm -rf node_modules dist *.log",
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run build && jasmine dist/**/*.spec.js",
    "build": "npm install && tsc",
    "watch": "npm run build && tsc --watch",
    "generate:pbjs": "pbjs -t static-module ../proto/*.proto -o dist/ground-protos.js",
    "generate:pbjson": "pbjs -t json ../proto/*.proto > dist/ground-protos.json",
    "generate:pbts": "pbts dist/ground-protos.js -o dist/ground-protos.d.ts"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "@google-cloud/firestore": "^7.7.0",
    "@ground/shared-protos": "file:../proto",
    "immutable": "^4.3.6"
  },
  "devDependencies": {
    "@types/jasmine": "^4.3.5",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-absolute-imports": "^0.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "gts": "^3.1.1",
    "jasmine": "^5.1.0",
    "prettier": "^2.8.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "4.8.4"
  }
}
